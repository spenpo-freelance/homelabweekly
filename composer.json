{
    "name": "spenpo-freelance/homelabweekly",
    "description": "wordpress automation",
    "type": "project",
    "require": {
        "roots/wordpress": "^6.8",
        "php": "^8.3",
        "wp-cli/wp-cli": "^2.12",
        "wpackagist-theme/astra": "^4.11.15",
        "spenpo-freelance/astra-homelabweekly": "dev-main",
        "aws/aws-sdk-php": "^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SpenpoFreelance\\HomelabWeekly\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Spenpo",
            "email": "spenpo@spenpo.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "roots/wordpress-core-installer": true,
            "composer/installers": true
        },
        "installer-paths": {
            "wordpress/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "wordpress/wp-content/themes/{$name}/": ["type:wordpress-theme"],
            "wordpress/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "path",
            "url": "src/themes/astra-homelabweekly"
        }
    ]
}
